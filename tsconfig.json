{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext", "ES2017", "es6"],
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": ["next-env.d.ts", ".next/types/**/*.ts", "**/*.ts", "**/*.tsx", "app/(home)/TruongHoc/(routesTruong)/[idTruong]/SoSanh/components/Example.js", "app/(SoSanh)/SoSanh/components/SS5S.jsx"],
  "exclude": ["node_modules"]
}
